<head>
    <title>Add-hanghoa</title>

    <%-include('../views/headercss') %>
</head>

<body>
    <%- include( '../views/navbar')%>
        <div class="container">
            <h1>THÊM HÀNG HÓA</h1>
            <form action="/chitiethanghoa" method="get">
                <table id="customers">
                    <tr>
                        <th rowspan="2" width="5%">STT</th>
                        <th rowspan="2" width="20%">Tên hàng</th>
                        <th rowspan="2" width="5%">Số lượng </th>
                        <th colspan="3 ">Giá vốn hàng bán (Giá kho)</th>
                        <th colspan="2 ">Doanh số (Giá bán)</th>
                        <th rowspan="2 " width="15%">Ghi chú </th>
                    </tr>
                    <tr>
                        <th>Đơn giá FOB <br> (EX-W) - (VNĐ)</th>
                        <th>Đơn giá kho <br>(VNĐ)</th>
                        <th>Đơn giá bán <br> (VNĐ)</th>
                        <th>Thành tiền <br>(VNĐ)</th>
                        <th>Doanh số <br>(Giá bán)</th>
                    </tr>
                    <tr>
                        <td><input class="form-control" type="text" name='stt'></td>
                        <td><input class="form-control" type="text" name='tenhang'></td>
                        <td><input class="form-control" type="text" name='soluong'></td>
                        <td><input class="form-control" type="text" name='dongiaFOB'></td>
                        <td><input class="form-control" type="text" name='dongiakho'></td>
                        <td><input class="form-control" type="text" name='dongiaban'></td>
                        <td><input class="form-control" type="text" name='thanhtien'></td>
                        <td><input class="form-control" type="text" name='doanhso'></td>
                        <td><input class="form-control" type="text" name='ghichu'></td>
                    </tr>
                </table>
                <input id="add-btn" class="btn btn-primary pull-right" type="submit" value="Save">
            </form>
        </div>
</body>